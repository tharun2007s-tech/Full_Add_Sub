onerror {resume}
quietly WaveActivateNextPane {} 0
add wave -noupdate /Full_Add_Sub/a
add wave -noupdate /Full_Add_Sub/b
add wave -noupdate /Full_Add_Sub/cin
add wave -noupdate /Full_Add_Sub/sum
